import matplotlib.pyplot as plt
import numpy as np



x=np.linspace(-3,3,15)
y=x**2
z=x**3
             


plt.plot(x,y,'g^')
plt.plot(x,y,'b--',linewidth=1.5,label='y=x**2')
plt.plot(x,z,'ro')
plt.plot(x,z,'g-',label='z=x**3')
plt.axis([-5,30,-15,30])
plt.title('My Curve Nature')
plt.xlabel('x-Coordinate')
plt.ylabel('y=x**2')
plt.grid(True)
plt.legend()
plt.show()


